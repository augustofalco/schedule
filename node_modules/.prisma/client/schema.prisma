generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Turno {
  id            Int      @id @default(autoincrement())
  dia           DateTime
  hora          String
  nombreClienta String
  observacion   String?
  createdAt     DateTime @default(now())
}
